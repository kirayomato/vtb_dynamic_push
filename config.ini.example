[scheduler]
# 是否根据活跃度调整轮询间隔 true/false
enable = true
# 单个ID最大权重
max_weight = 5

[image_proxy]
# 是否启用图片反代 true/false
enable = false
proxy_host = 127.0.0.1

[weibo]
# 是否启用动态推送 true/false
enable_dynamic_push = true
# 博主uid列表，使用英文逗号分隔
uid_list = 7842913991
# 是否启用cookies检测功能
enable_cookies_check = false
# 检测cookies使用的账号
cookies_check_uid = 1794972577
# 扫描间隔秒数，不建议设置太频繁，否则可能触发风控
intervals_second = 5


[bili]
# 是否启用动态推送 true/false
enable_dynamic_push = true
# 动态查询uid列表，使用英文逗号分隔
dynamic_uid_list = 1932862336
# 动态扫描间隔秒数，不建议设置太频繁，否则可能触发风控
dynamic_intervals_second = 5
# 是否启用开播推送 true/false
enable_living_push = true
# 开播查询uid列表，使用英文逗号分隔
live_uid_list = 1932862336
# 特别关注
special_list = 1932862336
# 直播扫描间隔秒数，不建议设置太频繁，否则可能触发风控
live_intervals_second = 30


[afd]
# 是否启用动态推送 true/false
